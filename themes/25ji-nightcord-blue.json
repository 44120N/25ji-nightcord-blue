	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#5F6A89"
			}
		},
		{
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#bd6a8b",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["variable", "meta.definition.variable.name", "support.variable"],
			"settings": {
				"foreground": "#ceae8e"
			}
		},
		{
			"scope": ["constant", "constant.numeric", "constant.language", "constant.character"],
			"settings": {
				"foreground": "#ddaacc"
			}
		},
		{
			"scope": ["entity.name.function", "support.function", "meta.function-call"],
			"settings": {
				"foreground": "#a2bbd9"
			}
		},
		{
			"scope": ["entity.name.class", "entity.name.type.class", "support.class", "support.type"],
			"settings": {
				"foreground": "#8eaaff"
			}
		},
		{
			"scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
			"settings": {
			"foreground": "#8a8acd"
			}
		},	  
		{
			"scope": ["string", "constant.other.symbol", "punctuation.definition.string"],
			"settings": {
				"foreground": "#bd6a8b"
			}
		},
		{
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"scope": ["entity.name.tag", "support.class.component.js", "support.class.component.tsx"],
			"settings": {
				"foreground": "#9BB8FF"
			}
		},
		{
			"scope": ["punctuation.definition.tag", "punctuation.separator.key-value"],
			"settings": {
				"foreground": "#ceae8e"
			}
		},
		{
			"scope": ["entity.other.attribute-name", "meta.tag.attributes", "support.attribute"],
			"settings": {
				"foreground": "#ceae8e"
			}
		},
		{
			"scope": ["meta.annotation", "markup.error"],
			"settings": {
				"foreground": "#884499"
			}
		},
		{
			"scope": ["keyword.operator", "keyword.other.else", "punctuation"],
			"settings": {
				"foreground": "#a6b4ef"
			}
		},
		{
			"scope": ["entity.name.enum", "support.constant"],
			"settings": {
				"foreground": "#ddaacc"
			}
		},
		{
			"scope": ["meta.decorator", "meta.decorator punctuation", "storage.type.annotation"],
			"settings": {
				"foreground": "#ddaacc"
			}
		},
		{
			"scope": ["support.type.property-name.json"],
			"settings": {
				"foreground": "#ddaacc"
			}
		},
		{
			"scope": ["markup.heading", "markup.bold", "markup.italic"],
			"settings": {
				"foreground": "#9BB8FF"
			}
		}
	]
}
